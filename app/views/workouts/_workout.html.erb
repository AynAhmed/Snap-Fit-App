<div id="<%= dom_id workout %>">
  <p>
    <strong>Name:</strong>
    <%= workout.name %>
  </p>

  <p>
    <strong>Duration:</strong>
    <%= workout.duration %>
  </p>

  <p>
    <strong>Calories burned:</strong>
    <%= workout.calories_burned %>
  </p>

  <p>
    <strong>Exercise type:</strong>
    <%= workout.exercise_type %>
  </p>

  <p>
    <strong>Log date:</strong>
    <%= workout.log_date %>
  </p>

  <p>
    <strong>User:</strong>
    <%= workout.user_id %>
  </p>

</div>
